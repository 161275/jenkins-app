{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "jenkins-app-taskdef-prod",
    "containerDefinitions": [
        {
            "name": "jenkins-app",
            "image": "nginx:1.26-alpine",
            "portMappings": [
                {
                "appProtocol": "http",
                "containerPort": 80,
                "hostPort": 80,
                "name": "nginx-80-tcp",
                "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::505679505304:role/ecsTaskExecutionRole"
}